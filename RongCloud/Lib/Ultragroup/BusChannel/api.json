    "url": "ultragroup/channel/create",
    "params": {
      "group": {
        "id": "10001",
        "busChannel": "name"
      }
    },
    "response":{
      "success": {
        "code": "200",
        "msg": "Successfully returned"
      },
      "fail": {
        "1002": {
          "code": "1002",
          "msg": "{{name}} is a required parameter"
        },
        "1501": {
          "code": "1501",
          "msg": "Incorrect parameter type, please check the parameter type, it should be {{type}} but currently is {{currentType}}"
        },
        "1004": {
          "code": "1004",
          "msg": "Signature error, please verify if Appkey and Secret are correct"
        },
        "1008": {
          "code": "1008",
          "msg": "Request frequency limit exceeded"
        },
        "1502": {
          "code": "1502",
          "msg": "Incorrect ban duration, currently passed as {{size}}, correct range is 1 - 1 * 30 * 24 * 60 minutes"
        }
      }
    }
