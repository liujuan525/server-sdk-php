{
  "stop": {
    "docs": "http://rongcloud.cn/docs/server.html#chatroom_message_stop_distribution",
    "url": "chatroom/message/stopDistribution",
    "params": {
      "chatroom": {
        "id": "chatroom01"
      }
    },
    "response": {
      "success": {
        "code": "200",
        "msg": "Successfully returned"
      },
      "fail": {
        "1005": {
          "code": "1005",
          "msg": "{{name}} exceeds the limit, {{name}} >= {{min}} and {{name}} <= {{max}}"
        },
        "1002": {
          "code": "1002",
          "msg": "{{name}} is a required parameter"
        },
        "1501": {
          "code": "1501",
          "msg": "{{name}} type is incorrect, please check the parameter type, it should be Object but {{currentType}} was passed"
        },
        "1004": {
          "code": "1004",
          "msg": "Signature error, please verify if Appkey and Secret are correct"
        },
        "1008": {
          "code": "1008",
          "msg": "Call frequency exceeded"
        }
      }
    }
  },
  "resume": {
    "docs": "http://rongcloud.cn/docs/server.html#chatroom_message_resume_distribution",
    "url": "chatroom/message/resumeDistribution",
    "params": {
      "chatroom": {
        "id": "chatroom01"
      }
    },
    "response": {
      "success": {
        "code": "200",
        "msg": "Successfully returned"
      },
      "fail": {
        "1005": {
          "code": "1005",
          "msg": "{{name}} exceeds the limit, {{name}} >= {{min}} and {{name}} <= {{max}}"
        },
        "1002": {
          "code": "1002",
          "msg": "{{name}} is a required parameter"
        },
        "1501": {
          "code": "1501",
          "msg": "Parameter type is incorrect, please check the parameter type, it should be {{type}} but {{currentType}} was passed"
        },
        "1004": {
          "code": "1004",
          "msg": "Signature error, please verify if Appkey and Secret are correct"
        },
        "1008": {
          "code": "1008",
          "msg": "Call frequency exceeded"
        }
      }
    }
  }
}